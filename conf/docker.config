docker.enabled         = true
process {
    // Process-specific containers
    withName:EXTRACT_ANNOT_MGX {
        container = 'chiamh/meta-omics_main:latest'
    } 
	withName:EXTRACT_ANNOT_MTX {
        container = 'chiamh/meta-omics_main:latest'
    }
	withName:ANNOT_MGX {
        container = 'chiamh/meta-omics_main:latest'
    }
	withName:ANNOT_MTX {
        container = 'chiamh/meta-omics_main:latest'
    }

}
